generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model music {
  id Int @id @default(autoincrement())
}

model memo {
  id      Int    @id @default(autoincrement())
  videoId String @db.VarChar(255)
  memo    String @db.LongText
}

model user {
  id          Int     @id @default(autoincrement())
  accessToken String? @db.VarChar(255)
}
